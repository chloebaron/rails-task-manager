<h1>TASKS SO MANY AH</h1>
<% @tasks.each do |task| %>
  <!-- <h3><%= link_to task.title, task_path(task)%> </h3> -->
  <div class="form-check">
    <input class="form-check-input" type="checkbox" value="" id="defaultCheck<% task.id %>">
    <label class="form-check-label" for="defaultCheck<% task.id %>">
      <h3><%= link_to task.title, task_path(task)%> </h3>
    </label>
  </div>
<% end %>



